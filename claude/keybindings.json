{
  "$schema": "https://www.schemastore.org/claude-code-keybindings.json",
  "$docs": "https://code.claude.com/docs/en/keybindings",
  "bindings": [
    {
      "context": "Chat",
      "bindings": {
        "ctrl+f": null,
        "meta+k": "chat:killAgents",
        "meta+p": null,
        "meta+m": "chat:modelPicker",
        "ctrl+_": null,
        "meta+u": "chat:undo",
        "ctrl+g": null,
        "meta+g": "chat:externalEditor",
        "ctrl+s": null,
        "meta+s": "chat:stash",
        "ctrl+v": null,
        "meta+v": "chat:imagePaste",
        "ctrl+q": null,
        "meta+q": "chat:snippetPicker"
      }
    }
  ]
}
